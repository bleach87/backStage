<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.bs.sql.auth">

    
    
    <select id="getUserInfo" resultType="UserVO">
	    SELECT 
	    	u.id          
			,u.email       
			,u.password    
			,u.name        
			,u.birthday    
			,u.phone       
			,u.created_date
			,u.level       
	    FROM USER u
		
	</select>
    
    
</mapper>